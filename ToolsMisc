	Name = "Webhook"
})




Tab:AddTextbox({
	Name = "DelayWebhook",
	Default = "Regenzy",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().Deley = Value
        Notification.new("info", "SetDelay Success", "Delay"..getgenv().Deley ,"2")
	end	  
})

Tab:AddButton({
	Name = "SendWebhook",
	Callback = function()
        while wait() do
            Webhook()
            wait(getgenv().Deley)
            Notification.new("success", "SendWebhook", "SendWebhook Success", "2")
        end
  	end    
})

Tab:AddTextbox({
	Name = "AmountCash",
	Default = "Regenzy",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().Cash = Value
        Notification.new("info", "SetAmountCash Success", "AmountCash"..getgenv().Cash ,"2")
	end	  
})


Tab:AddButton({
	Name = "LockCash",
	Callback = function()
        Notification.new("success", "Enabled LockCash Success", "AmountCash"..getgenv().Cash)
        while wait() do
            if getgenv().LockCash == true then
                if plr.leaderstats.Cash.Value >= tonumber(getgenv().Cash) then
                    Buytheme()
                    Webhookc()
                    wait(1)
                    game:Shutdown()
                    wait(10) -- à¹ƒà¸ªà¹ˆà¹€à¸‰à¸¢à¹†
                end
            end
        end
    end
})


