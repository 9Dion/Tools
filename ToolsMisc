--Text
function TextGame(texts)
    a = texts
    b = string.split(a,">")[3]
    c = string.split(b,"<")[1]
    --
    d = string.split(a," <")[1]
    Value = d.." "..c
    return Value
end
--Lavel
function GetLevel()
    level = game:GetService("Players").LocalPlayer.PlayerGui.Side.Frame.BakeryLevel.Level.Text
    levels = string.split(level,"'>")[2]
    Get = string.split(levels,"</")[1]
    return Get
end
